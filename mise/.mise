---
manage:
  - proj/*
  - etc
create:
  directories:
    - bin
    - doc
    - etc
    - lib
    - proj
    - src
    - var/tmp
    - .vim/colors
  links:
    - var/tmp: tmp
    - etc/mise/.mise: ~/.mise
    - etc/vim/.vimrc: ~/.vimrc
    - etc/vim/.vim: ~/.vim
    - etc/git/.gitconfig: ~/.gitconfig
    - etc/bash/.bashrc: ~/.bashrc

